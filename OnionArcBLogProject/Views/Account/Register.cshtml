@model User
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Forms - Ready Bootstrap Dashboard</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <link rel="stylesheet" href="~/BackTemplate/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/BackTemplate/https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i">
    <link rel="stylesheet" href="~/BackTemplate/assets/css/ready.css">
    <link rel="stylesheet" href="~/BackTemplate/assets/css/demo.css">
</head>
<body>
    <div class="wrapper">


        <div class="main-panel">
            <div class="content">

                <div class="container-fluid">
                    <form method="post" asp-action="Register" asp-controller="Account" asp-area="">
                        <div class="row">
                            <div class="offset-3 col-md-4">

                                <div class="card">
                                    <div class="card-header">
                                        <div class="card-title">Giriş Ekranı</div>
                                    </div>
                                    <div class="card-body">
                                         <div class="form-group">
                                            <label asp-for="FirstName">İsim</label>
                                            <input asp-for="FirstName" type="text" class="form-control" placeholder="İsim">
                                            
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="LastName">Soyisim</label>
                                            <input asp-for="LastName" type="text" class="form-control" placeholder="İsim">

                                        </div>
                                        <div class="form-group">
                                            <label asp-for="UserName">Soyisim</label>
                                            <input asp-for="UserName" type="text" class="form-control" placeholder="İsim">

                                        </div>
                                        <div class="form-group">
                                            <label asp-for="UserEmail">E-Posta</label>
                                            <input asp-for="UserEmail" type="email" class="form-control" placeholder="E-Posta">
                                        </div>
                                       
                                        <div class="form-group">
                                            <label asp-for="UserPassword">Şifre</label>
                                            <input asp-for="UserPassword" type="password" class="form-control" placeholder="Şifre">
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="UserPassword">Şifre Tekrar</label>
                                            <input asp-for="UserPassword" type="password" class="form-control" placeholder="Şifre">
                                        </div>
                                    </div>
                                    <div class="card-action mx-auto">
                                        <button type="submit" class="btn btn-primary">Kaydol</button>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <footer class="footer fixed-bottom">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul class="nav">
                            <li class="nav-item">
                                <a class="nav-link" href="http://www.themekita.com">
                                    ThemeKita
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">
                                    Help
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">
                                    Licenses
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <div class="copyright ml-auto">
                        2018, made with <i class="la la-heart heart text-danger"></i> by <a href="http://www.themekita.com">ThemeKita</a>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    </div>
    </div>
    <!-- Modal -->
    <div class="modal fade" id="modalUpdate" tabindex="-1" role="dialog" aria-labelledby="modalUpdatePro" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <h6 class="modal-title"><i class="la la-frown-o"></i> Under Development</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body text-center">
                    <p>Currently the pro version of the <b>Ready Dashboard</b> Bootstrap is in progress development</p>
                    <p>
                        <b>We'll let you know when it's done</b>
                    </p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</body>
<script src="~/BackTemplate/assets/js/core/jquery.3.2.1.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
<script src="~/BackTemplate/assets/js/core/popper.min.js"></script>
<script src="~/BackTemplate/assets/js/core/bootstrap.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/chartist/chartist.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/chartist/plugin/chartist-plugin-tooltip.min.js"></script>
@*<script src~/BackTemplate/="assets/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>*@
<script src="~/BackTemplate/assets/js/plugin/bootstrap-toggle/bootstrap-toggle.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/jquery-mapael/jquery.mapael.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/jquery-mapael/maps/world_countries.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/chart-circle/circles.min.js"></script>
<script src="~/BackTemplate/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>
<script src="~/BackTemplate/assets/js/ready.min.js"></script>
</html>